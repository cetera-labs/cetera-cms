    <key name="PRIMARY" unique="1">
        <column name="user_id" />
        <column name="external_type" />
    </key>
</table>

<table name="users_groups">
    <field name="id" type="int(11)" null="0" auto_increment="1" />
    <field name="name" type="varchar(100)" null="0" />
