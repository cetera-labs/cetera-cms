    <field name="text" type="text" null="1" />
    <key name="PRIMARY" unique="1">
        <column name="id" />
    </key>
    <key name="user_id">
        <column name="user_id" />
    </key>
    <key name="dat">
        <column name="dat" />
    </key>
    <key name="code">
        <column name="code" />
    </key>
</table>

<table name="vars">
    <field name="id" type="int(10) unsigned" null="0" auto_increment="1" />
